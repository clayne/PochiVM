define i32 @a_plus_b(i32 %_a_0, i32 %_b_1) {
entry:
  %a_0 = alloca i32
  %b_1 = alloca i32
  br label %body

body:                                             ; preds = %entry
  store i32 %_a_0, i32* %a_0
  store i32 %_b_1, i32* %b_1
  %0 = load i32, i32* %a_0
  %1 = icmp slt i32 %0, 0
  br i1 %1, label %then, label %else

then:                                             ; preds = %body
  %2 = load i32, i32* %a_0
  %3 = mul i32 %2, -1
  store i32 %3, i32* %a_0
  br label %afterIf

else:                                             ; preds = %body
  %4 = load i32, i32* %a_0
  %5 = mul i32 %4, 2
  store i32 %5, i32* %a_0
  br label %afterIf

afterIf:                                          ; preds = %else, %then
  %6 = load i32, i32* %a_0
  %7 = load i32, i32* %b_1
  %8 = add i32 %6, %7
  ret i32 %8
}
